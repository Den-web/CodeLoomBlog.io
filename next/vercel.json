{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://api.example.com"
    },
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
    "regions": ["fra1"],
    "functions": {
      "api/*.ts": {
        "runtime": "nodejs18.x"
      }
    }
  }